{
  "name": "zDemocracy-lowtech",
  "version": "1.0.0",
  "description": "Low tech experiment",
  "main": "distrib-electron/index.js",
  "scripts": {
    "start": "npm run start:node",
    "start:node": "node start-node.js",
    "nexe": "nexe",
    "exe:build": "npm run exe:build-linux && npm run exe:build-win && npm run exe:build-mac",
    "exe:build-linux": "nexe server.js -r \"views/**/*\" -t linux-x64-12.9.1 -o server-linux",
    "exe:build-win": "nexe server.js -r \"views/**/*\" -t windows-x64-12.9.1 -o server-win",
    "exe:build-mac": "nexe server.js -r \"views/**/*\" -t mac-x64-8.9.4 -o server-mac",
    "browser:build": "node ./distrib-browser/build.js",
    "browser:serve": "node ./distrib-browser/serve.js",
    "browser:deploy": "node ./distrib-browser/deploy.js",
    "electron:serve": "electron ./distrib-electron/index.js",
    "electron:forge:start": "electron-forge start",
    "electron:forge:package": "electron-forge package",
    "electron:forge:make": "electron-forge make"
  },
  "keywords": [],
  "author": "Antoine Cailly",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "browser-express": "^4.6.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "express-pouchdb": "^4.2.0",
    "localtunnel": "^2.0.0",
    "make-dir": "^3.1.0",
    "openpgp": "^4.10.9",
    "pouchdb": "^7.2.2",
    "pouchdb-upsert": "^2.2.0",
    "rss-parser": "^3.10.0",
    "twitter": "^1.7.1",
    "uuid": "^8.3.0"
  },
